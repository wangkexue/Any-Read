{
	"manifest_version": 2,

	"name": "Any-Read",
	"description": "A simple read list extension.",
	"version": "1.0",
	"icons": { "16": "images/icon16.png",
			   "48": "images/icon48.png",
			   "128": "images/icon128.png"},

	"permissions": [
		"tabs", 
		"storage",
		"contextMenus"
	],
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup": "popup.html"
	}
}